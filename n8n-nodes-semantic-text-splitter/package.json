{
  "name": "n8n-nodes-semantic-text-splitter",
  "version": "1.0.0",
  "description": "n8n community sub-node for Semantic Double-Pass Merging text splitting with embeddings-based chunking",
  "keywords": [
    "n8n-community-node-package",
    "text-splitter",
    "semantic-chunking",
    "embeddings",
    "ai",
    "langchain",
    "document-processing",
    "sub-node",
    "nlp",
    "machine-learning"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ResetNetwork/n8n-nodes#readme",
  "author": {
    "name": "Reset Network",
    "url": "https://github.com/ResetNetwork"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ResetNetwork/n8n-nodes.git",
    "directory": "n8n-nodes-semantic-text-splitter"
  },
  "bugs": {
    "url": "https://github.com/ResetNetwork/n8n-nodes/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && mkdir -p dist/nodes/text_splitters/TextSplitterSemanticDoublePass && cp -r nodes/text_splitters/TextSplitterSemanticDoublePass/*.svg dist/nodes/text_splitters/TextSplitterSemanticDoublePass/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes --ext .ts",
    "lintfix": "eslint nodes --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "files": [
    "dist",
    "index.js",
    "README.md",
    "LICENSE.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/text_splitters/TextSplitterSemanticDoublePass/TextSplitterSemanticDoublePass.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@langchain/core": "^0.1.0",
    "@langchain/textsplitters": "^0.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.25.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
