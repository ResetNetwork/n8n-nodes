{
  "name": "n8n-nodes-enhanced-query-retriever-rerank-tool",
  "version": "0.3.0",
  "description": "Advanced n8n community node with multi-query strategies, intelligent reranking, and comprehensive debugging for vector store retrieval",
  "keywords": [
    "n8n-community-node-package",
    "vector-store",
    "multi-query",
    "reranking",
    "ai",
    "langchain",
    "question-answer",
    "retrieval",
    "debugging",
    "enhanced"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ResetNetwork/n8n-nodes#readme",
  "author": {
    "name": "Reset Network",
    "url": "https://github.com/ResetNetwork"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ResetNetwork/n8n-nodes.git",
    "directory": "n8n-nodes-enhanced-query-retriever-rerank-tool"
  },
  "bugs": {
    "url": "https://github.com/ResetNetwork/n8n-nodes/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && mkdir -p dist/nodes/tools/EnhancedQueryRetrieverReRankTool && cp -r nodes/tools/EnhancedQueryRetrieverReRankTool/*.svg dist/nodes/tools/EnhancedQueryRetrieverReRankTool/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes/**/*.ts",
    "lintfix": "eslint nodes/**/*.ts --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/tools/EnhancedQueryRetrieverReRankTool/EnhancedQueryRetrieverReRankTool.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "n8n-workflow": "^1.25.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.25.1",
    "@langchain/core": "*",
    "langchain": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}