{
  "name": "n8n-nodes-recursive-language-model",
  "version": "0.1.0",
  "description": "n8n community tool node implementing Recursive Language Models (RLM) for processing unbounded context through REPL environments",
  "keywords": [
    "n8n-community-node-package",
    "rlm",
    "recursive",
    "language-model",
    "repl",
    "ai",
    "langchain",
    "tool"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ResetNetwork/n8n-nodes#readme",
  "author": {
    "name": "Reset Network",
    "url": "https://github.com/ResetNetwork"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ResetNetwork/n8n-nodes.git",
    "directory": "n8n-nodes-recursive-language-model"
  },
  "bugs": {
    "url": "https://github.com/ResetNetwork/n8n-nodes/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && mkdir -p dist/nodes/tools/RecursiveLanguageModel",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes",
    "lintfix": "eslint nodes --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/tools/RecursiveLanguageModel/RecursiveLanguageModel.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "n8n-workflow": "^1.25.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.25.1"
  },
  "dependencies": {
    "@langchain/core": "^1.0.1",
    "vm2": "^3.9.19"
  }
}

