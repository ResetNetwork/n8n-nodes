{
  "name": "n8n-nodes-collection",
  "version": "1.0.0",
  "description": "A collection of custom n8n nodes for enhanced document processing, text splitting, embeddings generation, advanced query retrieval with reranking, and extended trigger capabilities",
  "private": true,
  "workspaces": [
    "n8n-nodes-*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lintfix": "turbo lintfix",
    "format": "turbo format",
    "build:changed": "turbo build --filter='[HEAD~1]'",
    "test": "turbo test",
    "setup-local": "./setup-local.sh",
    "start-n8n": "./start-n8n.sh",
    "dev-watch": "./dev-watch.sh",
    "cleanup-local": "./cleanup-local.sh",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "publish-all": "npm publish --workspaces --access public",
    "test-sse-server": "node test-sse-server.js"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "document-processing",
    "text-splitting",
    "embeddings",
    "vector-store",
    "multi-query",
    "reranking",
    "ai",
    "langchain",
    "sse",
    "server-sent-events",
    "trigger",
    "real-time"
  ],
  "author": {
    "name": "Reset Network",
    "url": "https://github.com/ResetNetwork"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ResetNetwork/n8n-nodes.git"
  },
  "homepage": "https://github.com/ResetNetwork/n8n-nodes",
  "bugs": {
    "url": "https://github.com/ResetNetwork/n8n-nodes/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "@langchain/community": ">=0.3.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^5.0.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "turbo": "^2.5.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2"
  }
}
