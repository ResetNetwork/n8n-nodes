{
  "name": "n8n-nodes-mcp-client-extended",
  "version": "0.1.0",
  "description": "n8n community node for MCP Client Tool with dynamic headers support at execution time",
  "keywords": [
    "n8n-community-node-package",
    "mcp",
    "model-context-protocol",
    "ai",
    "langchain",
    "tools",
    "dynamic-headers"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ResetNetwork/n8n-nodes#readme",
  "author": {
    "name": "Reset Network",
    "url": "https://github.com/ResetNetwork"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ResetNetwork/n8n-nodes.git",
    "directory": "n8n-nodes-mcp-client-extended"
  },
  "bugs": {
    "url": "https://github.com/ResetNetwork/n8n-nodes/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && mkdir -p dist/nodes/McpClientExtended && cp nodes/McpClientExtended/mcp.svg nodes/McpClientExtended/mcp.dark.svg dist/nodes/McpClientExtended/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes --ext .ts",
    "lintfix": "eslint nodes --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.json nodes"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/McpClientExtended/McpClientExtended.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.11.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "n8n-workflow": "^1.25.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.25.1",
    "@modelcontextprotocol/sdk": "*",
    "@langchain/core": "*",
    "langchain": "*"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.2",
    "@langchain/core": "^0.3.0",
    "langchain": "^0.3.0",
    "zod": "^3.22.4"
  }
}
